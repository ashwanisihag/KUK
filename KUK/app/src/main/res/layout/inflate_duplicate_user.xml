<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/ll_duplicate_user"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_duplicate_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:padding="1dp">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="center">
            <TextView
                android:id="@+id/duplicate_user_institute"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Institute"
                android:textStyle="bold"/>
            <TextView
                android:id="@+id/duplicate_user_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="User Name"
                android:textStyle="bold"/>
           <!-- <TextView
                android:id="@+id/duplicate_user_RollNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Roll No"
                android:layout_marginTop="5dp"/>-->

            <TextView
                android:id="@+id/duplicate_user_batch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Batch"
                android:layout_marginTop="5dp"/>
            <TextView
                android:id="@+id/info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Touch for more details"
                android:textColor="@android:color/holo_red_dark"/>
                android:layout_marginTop="5dp"/>
        </LinearLayout>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/mdtp_red"/>

</LinearLayout>